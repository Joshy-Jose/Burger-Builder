Count":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"42"},{"filePath":"76","messages":"77","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"78","usedDeprecatedRules":"42"},{"filePath":"79","messages":"80","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/index.js",[],["81","82"],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/App.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/reportWebVitals.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/Layout/Layout.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/hoc/Aux.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/containers/BurgerBuilder/BurgerBuilder.js",["83"],"\nimport React,{ Component } from 'react';\nimport Aux from '../../hoc/Aux';\nimport Burger from  '../../components/Burger/Burger';\nimport classes from './BuildComponents.module.css';\nimport BuildControls from '../../components/Burger/BuildControls/BuildControls';\nimport Modal from '../../components/UI/Modal/Modal';\nimport OrderSummary from '../../components/OrderSummary/OrderSummary';\nconst INGREDIENT_PRICE = {\n    salad: 0.5,\n    cheese: 0.4,\n    meat: 1.3,\n    bacon: 0.7\n}\nclass BurgerBuilder extends Component\n{\n    \n     state = {\n         ingredients: {\n             salad: 0,\n             bacon: 0,\n             cheese: 0,\n             meat: 0\n         },\n         totalPrice : 4,\n         purchasable: false,\n         purchasing: false\n\n     }\n     updatePurchaseState(ingredients){\n         const sum = Object.keys(ingredients).map(igKey => {\n             return ingredients[igKey];\n         }).reduce((sum, el) => {\n             return sum + el;\n\n         },0);\n         this.setState({purchasable: sum > 0 });\n         console.log('purchasable',this.state.purchasable)\n\n     }\n\n     addIngredientHandler = (type) => {\n         \n        const oldCount = this.state.ingredients[type];\n        const updatedCount = oldCount + 1;\n        const updatedIngredients = {\n            ...this.state.ingredients\n        };\n        updatedIngredients[type] = updatedCount;\n        const priceAddition = INGREDIENT_PRICE[type];\n        const oldPrice = this.state.totalPrice;\n        const newPrice = oldPrice + priceAddition;\n\n        this.setState({\n            totalPrice: newPrice,\n            ingredients: updatedIngredients\n\n        })\n        this.updatePurchaseState(updatedIngredients);\n\n     }\n     \n    removeIngredientHandler = (type) => {\n        const oldCount = this.state.ingredients[type];\n        if(oldCount <= 0)\n        {\n            return;\n        }\n\n        const updatedCount = oldCount - 1;\n        const updatedIngredients = {\n            ...this.state.ingredients\n        };\n        updatedIngredients[type] = updatedCount;\n        const priceDeduction = INGREDIENT_PRICE[type];\n        const oldPrice = this.state.totalPrice;\n        const newPrice = oldPrice - priceDeduction;\n\n        this.setState({\n            totalPrice: newPrice,\n            ingredients: updatedIngredients\n\n        })\n        this.updatePurchaseState(updatedIngredients);\n         \n    }\n    purchaseHandler = () =>\n    {\n        this.setState({purchasing : true})\n    }\n    purchaseCancelHandler = () => \n    {\n        this.setState({purchasing:false});\n\n    }\n    purchaseContinueHandler = () => \n    {\n        alert('YOU CONTINUE !')\n    }\n\n    render() {\n        const disabledInfo = {\n            ...this.state.ingredients\n        };\n        for(let key in disabledInfo)\n        {\n            disabledInfo[key] = disabledInfo[key] <= 0\n        }\n        return (\n            <Aux>\n                <Modal show={this.state.purchasing} modalClosed={this.purchaseCancelHandler}>   \n                   <OrderSummary \n                   ingredients={this.state.ingredients}\n                   puchaseCancelled={this.purchaseCancelHandler}\n                   purchaseContinue={this.purchaseContinueHandler}\n                   price={this.state.totalPrice}/>\n                </Modal> \n                <Burger ingredients={this.state.ingredients}/>\n                <BuildControls\n                ingredientAdded={this.addIngredientHandler}\n                ingredientRemoved={this.removeIngredientHandler}\n                purchasing = {this.purchaseHandler}\n                disabled={disabledInfo}\n                price={this.state.totalPrice}\n                ordered={this.purchaseHandler}\n                />\n            </Aux>\n\n        );\n    }\n    \n\n}\nexport default BurgerBuilder;","/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/Burger/Burger.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/BurgerIngredients/BurgerIngredients.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/Burger/BuildControls/BuildControls.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/Burger/BuildControls/BuildControl/BuildControl.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/UI/Modal/Modal.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/OrderSummary/OrderSummary.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/UI/Modal/Backdrop/Backdrop.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/UI/Modal/Button/Button.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/Navigation/Toolbar/Toolbar.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/Logo/Logo.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/Navigation/Toolbar/NavigationItems/NavigationItems.js",[],"/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/Navigation/Toolbar/NavigationItems/NavigationItem/NavigationItem.js",["84"],"import { checkPropTypes } from 'prop-types';\nimport React from 'react';\nimport classes from './NavigationItem.module.css';\n\nconst navigationItem = (props) => (\n   <li className={classes.NavigationItem}>\n       <a href={props.link}\n       className={props.active ? classes.active : null}> \n          {props.children}\n        </a>\n    </li>\n);\n\nexport default navigationItem;","/media/joshy/1bb10890-6038-4a33-a125-4d6116ad8e30/BurgerBuilder/burger-builder/src/components/Navigation/Toolbar/SideDrawer/SideDrawer.js",[],{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","severity":1,"message":"90","line":5,"column":8,"nodeType":"91","messageId":"92","endLine":5,"endColumn":15},{"ruleId":"89","severity":1,"message":"93","line":1,"column":10,"nodeType":"91","messageId":"92","endLine":1,"endColumn":24},"no-native-reassign",["94"],"no-negated-in-lhs",["95"],"no-unused-vars","'classes' is defined but never used.","Identifier","unusedVar","'checkPropTypes' is defined but never used.","no-global-assign","no-unsafe-negation"]